<div class="show-more-wrap row">
    <div class="image col-xs-6"><img src="{image}" alt=""></div>
    <div class="content col-xs-6">
        <table>
            <tr>
                <td>Project:</td>
                <td><span id="project-name-git">{project_name}</span></td>
            </tr>
            <tr>
                <td>Theme name:</td>
                <td><span id="theme-name-git">{theme_name}</span></td>
            </tr>
            <tr>
                <td>Shop name:</td>
                <td>{shop_name}</td>
            </tr>
            <tr>
                <td>Prestashop version:</td>
                <td>{prestashop_vesion}</td>
            </tr>
            <tr>
                <td>Create:</td>
                <td>{shop_create}</td>
            </tr>
            <tr>
                <td>Data base:</td>
                <td><span id="data-base">{data_base}</span></td>
            </tr>
        </table>
        <h3>Modules</h3>
        <table>
            <tr>
                <td>Module</td>
                <td>Version</td>
            </tr>
            {modules}
        </table>
    </div>
</div>